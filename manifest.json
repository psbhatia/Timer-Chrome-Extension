{
	"manifest_version": 2,
	"name": "Timer",
	"version": "0.0.1",
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": ["content.js"]
		}
	],
	"permissions": [
		"background",
		"tabs",
		"storage"
	],
	
	"background": 
		{
		"scripts": ["background.js"]
	},
	"browser_action":{
		"default_icon": "logo.png",
		"default_popup": "popup.html"
	}

}